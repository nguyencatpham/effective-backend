{
  "name": "effective-study-language-rest-api",
  "version": "1.0.0",
  "description": "A Boilerplate application for building REST APIs using express, mysql in ES6",
  "main": "index.js",
  "author": "Phạm Nguyên Cát - Oscar",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nguyencatpham/effective-backend.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": " rimraf dist/ && webpack  --config webpack.config.js",
    "compile": "rimraf dist/ && babel index.js --out-dir dist && babel config --out-dir dist/config && babel src --out-dir dist/src",
    "start": "npm run compile && nodemon dist/index.js",
    "dev": "better-npm-run dev"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "asyncawait": "^1.0.6",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "body-parser": "^1.18.2",
    "camelcase-keys": "^4.1.0",
    "child_process": "^1.0.2",
    "compress": "^0.99.0",
    "compression": "1.6.2",
    "config": "^1.27.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cson": "^4.1.0",
    "debug": "^3.1.0",
    "dgram": "^1.0.1",
    "dotenv": "^4.0.0",
    "eslint-config-eslint": "^4.0.0",
    "express": "^4.16.2",
    "express-jwt": "5.1.0",
    "express-validation": "1.0.1",
    "express-winston": "^2.4.0",
    "fs": "^0.0.1-security",
    "helmet": "^3.9.0",
    "hjson": "^3.1.0",
    "http-status": "^1.0.1",
    "joi": "10.0.6",
    "jsonwebtoken": "7.1.9",
    "lodash": "^4.17.4",
    "method-override": "^2.3.10",
    "module": "^1.2.5",
    "mongoose": "4.7.4",
    "morgan": "^1.9.0",
    "mysql2": "^1.4.2",
    "net": "^1.0.2",
    "nodemon": "^1.12.1",
    "path": "^0.12.7",
    "pg-hstore": "^2.3.2",
    "properties": "^1.2.1",
    "request": "^2.83.0",
    "sequelize": "^4.17.2",
    "swagger-jsdoc": "^1.9.7",
    "tedious": "^2.0.0",
    "tls": "^0.0.1",
    "toml": "^2.3.3",
    "util": "^0.10.3",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "rimraf": "^2.6.2",
    "x2js": "^3.1.1"
  }
}
